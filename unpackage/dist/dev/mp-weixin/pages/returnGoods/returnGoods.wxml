<view><uni-form-add class="uni-form" vue-id="1" model="{{form}}" rules="{{rules}}" add-interface="AppReIn" bind:__l="__l" vue-slots="{{['default']}}"><view class="uni-form-item"><view class="uni-form-item__label"><view class="_sup">*</view>商品名称</view><view class="uni-form-item__content"><picker range="{{appGoodsList}}" value="{{appGoodsListIndex}}" range-key="name" data-event-opts="{{[['change',[['bindPickerChange2',['$event']]]]]}}" bindchange="__e"><view class="uni-input">{{appGoodsList[appGoodsListIndex].name}}</view></picker></view></view><view class="uni-form-item"><view class="uni-form-item__label"><view class="_sup">*</view>客户名称</view><view class="uni-form-item__content"><picker range="{{appCustomerList}}" value="{{appCustomerListIndex}}" range-key="auName" data-event-opts="{{[['change',[['bindPickerChange1',['$event']]]]]}}" bindchange="__e"><view class="uni-input">{{appCustomerList[appCustomerListIndex].auName}}</view></picker></view></view><view class="uni-form-item"><view class="uni-form-item__label"><view class="_sup">*</view>操作人</view><view class="uni-form-item__content"><picker range="{{apuNameList}}" value="{{apuNameListIndex}}" range-key="auName" data-event-opts="{{[['change',[['bindPickerChange',['$event']]]]]}}" bindchange="__e"><view class="uni-input">{{apuNameList[apuNameListIndex].auName}}</view></picker></view></view><view class="uni-form-item"><view class="uni-form-item__label"><view class="_sup">*</view>价格</view><view class="uni-form-item__content"><input class="uni-input__inner" type="number" placeholder="请输入价格" data-event-opts="{{[['input',[['__set_model',['$0','price','$event',[]],['form']]]]]}}" value="{{form.price}}" bindinput="__e"/></view></view><view class="uni-form-item"><view class="uni-form-item__label"><view class="_sup">*</view>数量</view><view class="uni-form-item__content"><input class="uni-input__inner" type="number" placeholder="请输入数量" data-event-opts="{{[['input',[['__set_model',['$0','count','$event',[]],['form']]]]]}}" value