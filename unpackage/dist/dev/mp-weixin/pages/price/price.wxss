.datetime {
  background: #fff;
  height: 80rpx;
  padding: 0 20rpx;
  font-size: 28rpx;
  color: #666;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.datetime text {
  color: #333;
  font-weight: 600;
}
.table-list {
  background: #fff;
  font-size: 24rpx;
  border-top: 1px solid #eee;
}
.table-list .table-list-head {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  color: #666;
  font-size: 26rpx;
  border-bottom: 1px solid #eee;
}
.table-list .table-list-head .item {
  padding: 0 10rpx;
  box-sizing: border-box;
  font-weight: 600;
}
.table-list .table-list-body .table-list-body-item {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  text-align: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  color: #333;
  font-size: 24rpx;
  border-bottom: 1px solid #eee;
}
.table-list .table-list-body .table-list-body-item .item {
  padding: 20rpx 10rpx;
  box-sizing: border-box;
}
.table-list .table-list-body .table-list-body-item .item3 {
  color: #888;
}
.table-list .item1 {
  width: 60%;
  text-align: left;
}
.table-list .item2 {
  width: 20%;
  border-left: 1px solid #eee;
  border-right: 1px solid #eee;
}
.table-list .item2 input {
  font-size: 24rpx;
  color: #000;
  padding: 0px;
}
.table-list .item3 {
  width: 20%;
}
.search-bar {
  padding: 10rpx 20rpx;
  box-sizing: border-box;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  background: #fff;
  border-bottom: 1px solid #eee;
}
.search-bar .search-bar-input {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  margin-right: 20rpx;
}
.search-bar .search-bar-input input {
  -webkit-appearance: none;
  background-color: #ccc;
  box-sizing: border-box;
  display: inline-block;
  height: 30px;
  line-height: 30px;
  outline: none;
  padding: 0 15px;
  -webkit-transition: border-color 0.2s cubic-bezier(0.645, 0.045, 0.355, 1);
  transition: border-color 0.2s cubic-bezier(0.645, 0.045, 0.355, 1);
  width: 100%;
  border: none;
  font-size: 30rpx;
  color: #fff;
  border-radius: 30px;
}
.search-bar .search-bar-btn {
  width: 90rpx;
}
.search-bar .search-bar-btn text {
  color: #fff;
  background-color: #409eff;
  border-color: #409eff;
  font-size: 12px;
  border-radius: 4px;
  width: 100%;
  height: 30px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}

